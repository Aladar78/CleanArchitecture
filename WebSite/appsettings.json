{
  "Logging": {
    "Debug": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning"
      },
      "FormatterOptions": {
        "UseUtcTimestamp": true,
        "TimestampFormat": "HH:mm:ss fff ",
        "IncludeScopes": true
      }
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  },
  "TransientFaultHandlingOptions": {
    "Enabled": true,
    "AutoRetryDelay": "00:00:09"
  },
  "Common": {
    "Solutions": {
      "Data": {
        "MainDB": {
          "ConnectionString": "Data Source=.;Initial Catalog=MainDB-Prod;Integrated Security=True;Trust Server Certificate=True"
        }
      }
    }
  },
  "AllowedHosts": "*"
}
